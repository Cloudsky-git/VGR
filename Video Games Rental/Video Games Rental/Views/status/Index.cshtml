@model IEnumerable<Video_Games_Rental.Models.status>

@{

    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_EntryLayout.cshtml";
}

<h2>Index</h2>
@if ((string)Session["myRole"] == "admin")
{
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.status1)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.status1)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.status_id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.status_id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.status_id })
                </td>
            </tr>
        }

    </table>
}